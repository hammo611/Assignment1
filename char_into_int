#include <stdio.h>

int main()
{
   char test[5] = {'t','e','s','t'};
   int key = 3;
   int i = 0;
   int num[i];
   
   
   while(i < 4)
   {
       num[i] = (int)test[i];
       num[i] = num[i] + key;
       printf("%c\n", (char)(num[i]));
       i++;
    }
    
    return 0;
}


